@model Lincoln.Admin.Web.Models.ExamLeftPanelViewModel

<div id="navMainPanel" class="casLftPanelLayout">
    <!--<div id="timePanel" class="casLftPanelSect1 hideSection">
        <div class="casLftPanelBaseField casLftTimePnl">
            <span id="lblTimeRemaining" class="timeLabel italic">Time Remaining</span>
            <a id="toggleTime" class="collapsePnlHover far fa-minus-square"></a>
        </div>
        <div id="showTimePanel" class="casLftPanelBaseField clockField medWeight clockFieldTimePanel">
            <input name="txtTimer" type="text" maxlength="8" size="6" readonly="readonly" class="casLftPanelBaseField clockField medWeight" id="txtTimer">
            <input type="hidden" name="jsTimeSpent" id="jsTimeSpent">
            <input type="hidden" name="jsTotalTimeSpent" id="jsTotalTimeSpent">
        </div>
    </div>-->
    <!--<div id="timePanelDivider" class="casLftPanelSectDivider hideSection"></div>-->
    <div id="allAnsweredPanel" class="casLftPanelSect22">
        <a id="toggleAnswered" class="collapsePnlHover fa fa-minus-square"></a>
        <div id="answeredPanel">
            <div id="NotAnswered" class="casLftPanelBaseField actionLink">
                <a onclick="return false;" id="lblUnansweredValue" href="javascript:__doPostBack('lblUnansweredValue','')">@(Convert.ToInt32(Model.QuestionTotal) - Convert.ToInt32(Model.AnswerTotal))</a>
            </div>
            <br>
            <div class="casLftPanelBaseField descLabel descLabelTop">Not Answered</div>

            <div style="clear:both"></div>
            <div id="ShowUnAnsweredPanel" class="ansPanel" style="display: none;">
                <div class="ansHd">
                    <div class="ansHdLft alignLeft">
                        @(Model.QuestionTotal - Model.AnswerTotal) Unanswered
                    </div>
                    <div class="ansHdRht">
                        <div id="CloseUnAnsweredPanel" class="closeButton" style="cursor: pointer;"><div class="casLftPanelBaseField closeColor icon2 smarrowleft iconSizeSSSm"></div><div class="closeButtonLabel">Close</div></div>
                    </div>
                </div>
                <div class="ansBody alignLeft">
                    <div id="questions_row_1" class="ansRow">
                        @if (Model.NotAnsweredList != null && Model.NotAnsweredList.Count() > 0)
                        {
                            foreach (var item in Model.NotAnsweredList)
                            {
                                <a id="@item" onclick="goTo('@item','@Model.CoureseID');" class="goTo"> <span class="directNavElement narrow rowSpace icon2 question elementOn"> <span class="directNavLabel shiftLft  labelOn"> @item</span></span></a>
                            }
                        }

                    </div>
                </div>
            </div>
            <div class="casLftPanelSectDivider subDivider"></div>

            <div style="clear:both;"></div>
            <div id="ShowAnsweredPanel" class="ansPanel" style="display: none;">
                <div class="ansHd">
                    <div class="ansHdLft alignLeft">
                        @Model.AnswerTotal Answered
                    </div>
                    <div class="ansHdRht">
                        <div id="CloseAnsweredPanel" class="closeButton" style="cursor: pointer;"><div class="casLftPanelBaseField closeColor icon2 smarrowleft iconSizeSSSm"></div> <div class="closeButtonLabel">Close</div></div>
                    </div>
                </div>
                <div class="ansBody alignLeft">
                    <div id="questions_row_1" class="ansRow">
                        @if (Model.CompletedList != null && Model.CompletedList.Count() > 0)
                        {
                            foreach (var item in Model.CompletedList)
                            {
                                <a id="@item" onclick="goTo('@item','@Model.CoureseID');" class="goTo"> <span class="directNavElement narrow rowSpace icon2 question elementOn"> <span class="directNavLabel shiftLft  labelOn"> @item</span></span></a>
                            }
                        }

                    </div>
                </div>
            </div>

            <div id="Answered" class="casLftPanelBaseField actionLink answeredLblTop">
                <a onclick="return false;" id="lblAnsweredValue" href="javascript:__doPostBack('lblAnsweredValue','')">@Model.AnswerTotal</a>
            </div>
            <br>
            <div id="toggleAnsweredPanel" class="casLftPanelBaseField descLabel descLabelTop">Completed</div>
        </div>
        <div id="totalAnswersPanel" class="totalAnswers">
            <div id="Total" class="casLftPanelBaseField actionLink noUnderline totalAnsweredLblTop">
                <span id="lblTotal" class="totalAnswers">@Model.QuestionTotal</span>
            </div>
            <br>
            <div class="casLftPanelBaseField descLabel descLabelTop">Questions</div>
        </div>
    </div>
    <div id="flagPanelDivider" class="casLftPanelSectDivider"></div>
    <div id="flaggedPanel" class="casLftPanelSect2 casLftPanelSect3">
        <div class="casLftPanelBaseField flagImgPad">
            <a id="btnFlag" onclick="flagCurrentQuestion()" title="Flag Question" class="fa fa-flag iconSizeSm noUnderline passLink"></a>
        </div>
        <br>

        <div style="clear:both;"></div>
        <div id="ShowFlaggedAnsweredPanel" class="ansPanel" style="display: none;">
            <div class="ansHd">
                <div class="ansHdLft alignLeft">
                    @Model.FlagTotal Flagged
                </div>
                <div class="ansHdRht">
                    <div id="CloseFlaggedAnsweredPanel" class="closeButton" style="cursor: pointer;"><div class="casLftPanelBaseField closeColor icon2 smarrowleft iconSizeSSSm"></div> <div class="closeButtonLabel">Close</div></div>
                </div>
            </div>
            <div class="ansBody alignLeft">
                <div id="questions_row_1" class="ansRow">
                    @if (Model.FlagedList != null && Model.FlagedList.Count() > 0)
                    {
                        foreach (var item in Model.FlagedList)
                        {
                            <a id="@item" onclick="goTo('@item','@Model.CoureseID');" class="goTo"> <span class="directNavElement narrow rowSpace icon2 question elementOn"> <span class="directNavLabel shiftLft  labelOn"> @item</span></span></a>
                        }
                    } </div>
            </div>
        </div>
        @*<a id="btnPrevFlag" class="casLftPanelBaseField altColor icon2 smarrowleft iconSizeSSm noUnderline" onclick="goToPrevFlag()" title="Previously flagged question"></a>*@
        <div id="Flagged" class="casLftPanelBaseField actionLink flagActLnk">
            <a onclick="return false;" id="lblFlaggedValue" href="javascript:__doPostBack('lblFlaggedValue','')">@Model.FlagTotal</a>
        </div>
        @*<a id="btnNextFlag" class="casLftPanelBaseField altColor icon2 smarrowright iconSizeSSm noUnderline" onclick="goToNextFlag()" title="Next flagged question"></a>*@
        <br>
        <div class="casLftPanelBaseField descLabel descLabelTop1">Flagged Questions</div>
    </div>
    <div class="casLftPanelSectDivider"></div>
    <div id="helperPanel" class="casLftPanelSect4">
        <a id="lnkCalc" class="noUnderline" onclick="ShowCalculator();return false;">
            <span class="casLftPanelBaseField fa fa-calculator iconSizeMed descLabelTop1"></span>
        </a>
        <div id="btnCalcContent" class="casLftPanelBaseField descLabel helperContent calcRow">
            <a onclick="ShowCalculator();return false;" id="btnCalc" title="Display Calculator" class="defaultText" href="javascript:__doPostBack('btnCalc','')">Calculator</a>
        </div>
        <a id="lnkHelp" onclick="ShowHelp(helpSrcUrl);return false;" class="noUnderline noOutline">
            <span class="casLftPanelBaseField fa fa-question-circle iconSizeLrg helpPaddingLnk"></span>
        </a>
        <br>
        <div class="casLftPanelBaseField descLabel helperContent helpPadding">
            <a onclick="ShowHelp();return false;" id="btnHelp" title="Display Help System" class="defaultText" href="javascript:__doPostBack('btnHelp','')">Help</a>
        </div>
    </div>
    <div id="takeABreak">
        <div class="casLftPanelSectDivider"></div>
        <div id="breakPanel" class="casLftPanelSect7 casLftPanelBaseField flagImgPad">
            <a id="lnkTakeABreak" onclick="TakeABreak();" class="passLink" title="Take A Break">
                <span class="fa fa-hand-paper-o iconSizeSm noUnderline"></span>
            </a>
            <br>
            <a onclick="TakeABreak(); return false;" id="btnTakeABreak" title="Take A Break" class="defaultText descLabel" href="javascript:__doPostBack('btnTakeABreak','')">Take a Break</a>
        </div>
    </div>
</div>

@model IEnumerable<Lincoln.Admin.Web.Models.AdminOnlineExaminationViewModel>
@{

    int sl = 1;
}
<table class="table table-striped table-bordered base-style" id="dataTables-example" style="width:100%">
    <thead>
        <tr>
            <th>Sl No</th>
            <th>Student Name</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {


            foreach (var item in Model)
            {
                <tr>
                    <td>@sl</td>
                    <td>@item.StudentName</td>
                    <td>@item.Status</td>
                   
                </tr>
                sl++;
            }
        }

    </tbody>
</table>
<script>
    $('#dataTables-example').dataTable();

</script>



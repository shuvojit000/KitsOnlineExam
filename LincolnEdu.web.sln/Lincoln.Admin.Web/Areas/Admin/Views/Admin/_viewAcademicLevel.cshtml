@model Lincoln.Admin.Web.Models.AcademicViewModel

<div class="panel-body">
    <!--main content start-->
    <div class="row">
        <div class="col-lg-12">
            <!--PAGE CONTENT BEGINS-->
            <div class="modal-body">
                <label><b>Academic Code:</b> </label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                        @Html.DisplayFor(a => a.AcademicCode, new { @class = "form-control" })


                    </div>
                </div>
                <label><b>Academic Name: </b></label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                        @Html.DisplayFor(a => a.AcademicName, new { @class = "form-control" })

                    </div>
                </div>
            </div>

            <!--PAGE CONTENT ENDS-->
        </div>
    </div>
    <!--main contant end-->
</div>




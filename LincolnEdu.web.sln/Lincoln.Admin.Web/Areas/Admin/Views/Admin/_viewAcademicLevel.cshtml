@model Lincoln.Admin.Web.Models.AcademicViewModel

<div class="panel-body">
    <!--main content start-->
    <div class="row">
        <div class="col-lg-12">
            <!--PAGE CONTENT BEGINS-->
            <div class="modal-body">
                <div class="form-group">
                    <label><b>Academic Code:</b> </label>
                    @Html.DisplayFor(a => a.AcademicCode, new { @class = "form-control" })
                </div>

                <div class="form-group">
                    <label><b>Academic Level: </b></label>
                    @Html.DisplayFor(a => a.AcademicName, new { @class = "form-control" })
                </div>
            </div>

            <!--PAGE CONTENT ENDS-->
        </div>
    </div>
    <!--main contant end-->
</div>




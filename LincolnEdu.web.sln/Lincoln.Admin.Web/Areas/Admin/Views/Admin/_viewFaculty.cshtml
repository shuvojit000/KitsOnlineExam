@model Lincoln.Admin.Web.Models.FacultyViewModel

<div class="panel-body">
    <!--main content start-->
    <div class="row">
        <div class="col-lg-12">
            <!--PAGE CONTENT BEGINS-->
            <div class="modal-body">
                <div class="row">
                    <div class="form-group col-lg-6">
                        <label>Employee Code: </label>
                        <div class="position-relative has-icon-left">
                            @Html.TextBoxFor(a => a.EmployeeCode, new { @class = "form-control", @readonly = "readonly" })
                            <div class="form-control-position">
                                <i class="ft-user"></i>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-lg-6">
                        <label>Password: </label>
                        <div class="position-relative has-icon-left">
                            @Html.TextBoxFor(a => a.Password, new { @class = "form-control", @readonly = "readonly" })
                            <div class="form-control-position">
                                <i class="ft-user"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-lg-12">
                        <label>Employee Name: </label>
                        <div class="position-relative has-icon-left">
                            @Html.TextBoxFor(a => a.EmployeeName, new { @class = "form-control", @readonly = "readonly" })
                            <div class="form-control-position">
                                <i class="ft-user"></i>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-lg-6">
                        <label>Mobile No: </label>
                        <div class="position-relative has-icon-left">
                            @Html.TextBoxFor(a => a.MobileNo, new { @class = "form-control", @readonly = "readonly" })
                            <div class="form-control-position">
                                <i class="fa fa-mobile"></i>
                            </div>
                        </div>
                    </div>
                    <div class="form-group col-lg-6">
                        <label>Email Id: </label>
                        <div class="position-relative has-icon-left">
                            @Html.TextBoxFor(a => a.EmailID, new { @class = "form-control", @readonly = "readonly" })
                            <div class="form-control-position">
                                <i class="ft-mail"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">

            </div>
            <!--PAGE CONTENT ENDS-->
        </div>
    </div>
    <!--main contant end-->
</div>

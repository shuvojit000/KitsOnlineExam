@model Lincoln.Admin.Web.Models.ProgrammeSemesterViewModel
<div class="panel-body">
    <!--main content start-->
    <div class="row">
        <div class="col-lg-12">
            <!--PAGE CONTENT BEGINS-->
            <div class="modal-body">
                <div class="row">
                    <div class="form-group col-lg-6">
                        <label><b>Department: </b></label>
                        <div class="position-relative has-icon-left">
                            @Html.DisplayFor(a => a.DepartmentName, new {  @class = "form-control"  })

                        </div>
                    </div>
                    <div class="form-group col-lg-6">
                        <label><b>Programme :</b> </label>
                        <div class="position-relative has-icon-left">
                            @Html.DisplayFor(a => a.ProgrammeName, new { @class = "form-control" })
                        </div>
                        </div>

                        </div>
                    </div>
                <div class="row">
                    
                    <div class="form-group col-lg-6">
                        <label><b>Programme year:</b> </label>
                        <div class="position-relative has-icon-left">
                            @Html.DisplayFor(a => a.ProgrammeYear,  new {  @class = "form-control" })

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="form-group col-lg-6">
                        <label><b>Programme Semester:</b> </label>
                        <div class="position-relative has-icon-left">
                            @Html.DisplayFor(a => a.ProgrammeSemester, new { @class = "form-control" })

                        </div>
                    </div>

                    <div class="form-group col-lg-6">
                        <label><b>Semester Type:</b> </label>
                        <div class="position-relative has-icon-left">
                            @Html.DisplayFor(a => a.SemesterType, new { @class = "form-control" })

                        </div>
                    </div>
                </div>

            </div>
           
            <!--PAGE CONTENT ENDS-->
        </div>
    </div>
    <!--main contant end-->
</div>



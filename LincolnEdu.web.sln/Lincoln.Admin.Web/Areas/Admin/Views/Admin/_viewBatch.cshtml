@model Lincoln.Admin.Web.Models.BatchViewModel

<div class="panel-body">
    <!--main content start-->
    <div class="row">
        <div class="col-lg-12">
            <!--PAGE CONTENT BEGINS-->
            <div class="modal-body">
                <label><b>Batch Name:</b> </label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                        @Html.DisplayFor(a => a.BatchName, new { @class = "form-control" })


                    </div>
                </div>
                <label><b>Batch Desc: </b></label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                        @Html.DisplayFor(a => a.BatchDesc, new { @class = "form-control" })

                    </div>
                </div>
            </div>

            <!--PAGE CONTENT ENDS-->
        </div>
    </div>
    <!--main contant end-->
</div>





@model IEnumerable<Lincoln.Admin.Web.Models.ProgramVersioningViewModel>
@{

    int sl = 1;
}

<table class="cell-border stripe" id="dataTables-example" style="width:100%">
    <thead>
        <tr>
            <th>SlNo</th>
            <th>Check</th>
            <th>Version</th>
            <th>Department Name</th>
            <th>Program Name</th>
            <th>Place Holder</th>
            <th>Credit</th>
            <th>STATUS</th>
            <th>ACTION</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {


            foreach (var item in Model)
            {
                <tr>
                    <td>@sl</td>
                    <td>
                        <center>
                            <input type="checkbox" class="statusCheck" data-id="@item.ProgramVersioningID">
                        </center>
                    </td>
                    <td>@item.Version</td>
                    <td>@item.DepartmentName</td>
                    <td>@item.ProgramName</td>
                    <td>@item.PlaceHolder</td>
                    <td>@item.Credit</td>
                    <td>@item.Active</td>

                    <td>
                        <a href="javascript:void(0);" onclick="fnViewProgramVersioning(@item.ProgramVersioningID)" title="View"><i class="fa fa-eye"></i></a> &nbsp;
                        <a href="javascript:void(0);" onclick="fnAddProgramVersioning(@item.ProgramVersioningID)" title="Edit"><i class="fa fa-edit"></i></a> &nbsp;
                        <a href="javascript:void(0);" onclick="fnDeleteProgramVersioning(@item.ProgramVersioningID)" title="Delete"><i class="fa fa-trash"></i></a>
                    </td>
                </tr>
               // @sl= @sl + 1;
            }
        }
    </tbody>
</table>
<script>
        $('#dataTables-example').dataTable({
        dom: 'Bfrtip', buttons: [
            {
                extend: 'excelHtml5',
                exportOptions: {
                    columns: [0, 2, 3, 4, 5, 6, 7]
                }
            },
            {
                extend: 'pdfHtml5',
                exportOptions: {
                    columns: [0, 2, 3, 4, 5, 6, 7]
                }
            }, {
                extend: 'print',
                exportOptions: {
                    columns: [0, 2, 3, 4, 5, 6, 7]
                }
            }]
    });</script>
@model Lincoln.Admin.Web.Models.CourseViewModel

<div class="panel-body">
    <!--main content start-->
    <div class="row">
        <div class="col-lg-12">
            <!--PAGE CONTENT BEGINS-->
            <div class="modal-body">
                <label>Course Code: </label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                        @Html.DisplayFor(a => a.CourseCode, new { @class = "form-control" })

                       
                    </div>
                </div>
                <label>Course Name: </label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                        @Html.DisplayFor(a => a.CourseName, new { @class = "form-control" })
                        
                    </div>
                </div>
            </div>
            
            <!--PAGE CONTENT ENDS-->
        </div>
    </div>
    <!--main contant end-->
</div>


@model Lincoln.Admin.Web.Models.SubjectViewModel

<div class="panel-body">
    <!--main content start-->
    <div class="row">
        <div class="col-lg-12">
            <!--PAGE CONTENT BEGINS-->
            <div class="modal-body">
                <label>Subject Code: </label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                       @Html.DisplayFor(a=>a.SubjectCode,new { @class="form-control"})
                        
                    </div>
                </div>
                <label>Subject Name: </label>
                <div class="form-group">
                    <div class="position-relative has-icon-left">
                        @Html.DisplayFor(a => a.SubjectName, new { @class = "form-control" })
                       
                    </div>
                </div>
            </div>
            
            <!--PAGE CONTENT ENDS-->
        </div>
    </div>
    <!--main contant end-->
</div>

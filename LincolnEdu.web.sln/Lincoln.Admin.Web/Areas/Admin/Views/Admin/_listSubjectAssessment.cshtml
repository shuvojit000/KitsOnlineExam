


@model IEnumerable<Lincoln.Admin.Web.Models.SubjectAssessmentViewModel>
@{

    int sl = 1;
}

<table class="table table-striped table-bordered base-style" id="dataTables-example" style="width:100%">
    <thead>
        <tr>
            <th>SlNo</th>
            <th>Check</th>
            <th>Faculty</th>
            <th>Program</th>
            <th>Syllabus Version</th>
            <th>Semister</th>
            <th>Course</th>
            <th>Academic Year</th>

            <th>Status</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.Count() > 0)
        {


            foreach (var item in Model)
            {
        <tr>
            <td>@sl</td>
            <td>
                <center>
                    <input type="checkbox" class="statusCheck" data-id="@item.SubjectAssessmentID">
                </center>
            </td>

            <td>@item.FacultyName</td>
            <td>@item.ProgramName</td>
            <td>@item.SyllabusVersionName</td>
            <td>@item.SemisterName</td>
            <td>@item.CourseName</td>
            <td>@item.AcademicYearCode</td>

            <td>@item.Active</td>

            <td>
                <a href="javascript:void(0);" onclick="fnViewProgramVersioning(@item.SubjectAssessmentID)" title="View"><i class="fa fa-eye"></i></a> &nbsp;
                <a href="javascript:void(0);" onclick="fnAddProgramVersioning(@item.SubjectAssessmentID)" title="Edit"><i class="fa fa-edit"></i></a> &nbsp;
                <a href="javascript:void(0);" onclick="fnDeleteProgramVersioning(@item.SubjectAssessmentID)" title="Delete"><i class="fa fa-trash"></i></a>
            </td>
        </tr>
                // @sl= @sl + 1;
            }
        }
    </tbody>
</table>
